{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Adilson Junior <aj.adilsonjunior1@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/src/index.js",
    "dev": "tsnd --transpile-only --respawn --ignore-watch node_modules src/server.ts --ignore ./__tests__",
    "lint": "eslint ./src ./__tests__ --ext .ts",
    "lint:fix": "eslint ./src ./__tests__ --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' '__tests__/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' '__tests__/**/*.ts'"
  },
  "devDependencies": {
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "prettier": "^2.1.2",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "module-alias": "^2.2.2"
  }
}
